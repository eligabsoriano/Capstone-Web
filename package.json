    "react-dom": "^19.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.70.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
